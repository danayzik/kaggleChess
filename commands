g++ -std=c++17 -O2 -DNDEBUG -o chessBot main.cpp negaMaxQSearcher.cpp game_tables.cpp heuristic_evaluator.cpp engine.cpp transposition_table.cpp sorters.cpp  -ffunction-sections -fdata-sections -flto
strip chessBot



g++ -std=c++17 -pg -g -O2 -o chessBot main.cpp negaMaxQSearcher.cpp game_tables.cpp heuristic_evaluator.cpp engine.cpp transposition_table.cpp sorters.cpp -ffunction-sections -fdata-sections

g++ -std=c++17 -O2 -DNDEBUG -mconsole -o compare_evaluators compare_evaluators.cpp game_tables.cpp heuristic_evaluator.cpp



